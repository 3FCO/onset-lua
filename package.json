{
	"name": "vscode-olua",
	"description": "Unofficial Onset Lua language support for Visual Studio Code",
	"version": "1.0.0",
	"engines": {
		"vscode": "^0.10.1"
	},
	"icon": "images/logo.png",
	"publisher": "zoukidev",
	"categories": [
		"Languages",
		"Snippets"
	],
	"contributes": {
		"snippets": [
			{
				"language": "olua",
				"path": "./snippets/enums.json"
			},
			{
				"language": "olua",
				"path": "./snippets/client_events.json"
			},
			{
				"language": "olua",
				"path": "./snippets/server_events.json"
			},
			{
				"language": "olua",
				"path": "./snippets/client_functions.json"
			},
			{
				"language": "olua",
				"path": "./snippets/server_functions.json"
			}
		],
		"languages": [
			{
				"id": "olua",
				"aliases": [
					"OLua"
				],
				"extensions": [
					".lua"
				],
				"configuration": "./language-configuration.json"
			}
		],
		"grammars": [
			{
				"language": "olua",
				"scopeName": "source.lua",
				"path": "./syntaxes/lua.tmLanguage"
			}
		]
	},
	"repository": {
		"type" : "git",
		"url" : "https://github.com/zoukidev/vscode-olua"
	}
}